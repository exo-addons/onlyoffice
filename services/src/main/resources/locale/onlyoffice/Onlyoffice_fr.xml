<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!-- For menu caption in Action Bar -->
  <UIActionBar>
    <tooltip>
      <OnlyofficeOpen>Modifier dans OnlyOffice</OnlyofficeOpen>
    </tooltip>
  </UIActionBar>
  <!-- Used in Administration-Content-Explorer in Views settings a popup with actions list -->
  <UITabForm>
    <label>
      <onlyofficeOpen>Modifier dans OnlyOffice</onlyofficeOpen>
    </label>
  </UITabForm>

  <!-- Messages -->
  <OnlyofficeEditor>
    <message>
      <ViewerNotFound>OnlyOffice document editor not found</ViewerNotFound>
    </message>
    <error>
      <EditorCannotBeCreated>Editor cannot be created. Please retry.</EditorCannotBeCreated>
      <CannotReadDocument>Cannot read the document. Please retry.</CannotReadDocument>
      <CannotCreateEditor>Cannot create editor. Please retry.</CannotCreateEditor>
      <CannotSendEditorConfiguration>Cannot send editor configuration. Please retry.</CannotSendEditorConfiguration>
      <CannotInitEditorConfiguration>Cannot initialize editor configuration. Please retry.</CannotInitEditorConfiguration>
      <InitializationError>Error initialize editor. Please retry.</InitializationError>
      <DocumentIdRequired>Wrong request: document ID required.</DocumentIdRequired>
    </error>
  </OnlyofficeEditor>
</bundle>