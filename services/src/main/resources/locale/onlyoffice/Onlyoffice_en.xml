<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!-- For menu caption in Action Bar -->
  <UIActionBar>
    <tooltip>
      <NewDocument>New Document</NewDocument>
      <OnlyofficeOpen>Edit Online</OnlyofficeOpen>
      <OnlyofficeClose>Close Online Editor</OnlyofficeClose>
    </tooltip>
  </UIActionBar>
  <!-- Used in Administration-Content-Explorer in Views settings a popup with actions list -->
  
  <UIPopupWindow>
    <title>
      <UINewDocumentForm>New Document</UINewDocumentForm>
    </title>
  </UIPopupWindow>
  
  <UINewDocumentForm>
    <action>
      <Cancel>#{word.cancel}</Cancel>
      <Save>Create Document</Save>
    </action>
    <label>
      <titleTextBox>Document Name:</titleTextBox>
      <typeSelectBox>Document Type</typeSelectBox>
    </label>
  </UINewDocumentForm>  
  <!-- Messages -->
  <OnlyofficeEditor>
    <message>
      <ViewerNotFound>Online document editor not found</ViewerNotFound>
    </message>
    <error>
      <EditorCannotBeCreated>Editor cannot be created. Please retry.</EditorCannotBeCreated>
      <CannotReadDocument>Cannot read the document. Please retry.</CannotReadDocument>
      <CannotCreateEditor>Cannot create editor. Please retry.</CannotCreateEditor>
      <CannotSendEditorConfiguration>Cannot send editor configuration. Please retry.</CannotSendEditorConfiguration>
      <InitializationError>Error initialize editor. Please retry.</InitializationError>
      <DocumentIdRequired>Wrong request: document ID required.</DocumentIdRequired>
    </error>
  </OnlyofficeEditor>
  <portal>
    <intranet>
      <oeditor>Online Document Editor</oeditor>
    </intranet>
  </portal>
</bundle>